import java.util.Scanner;

public class Main {
  public static void main(String[] args) {


  
    Scanner input = new Scanner(System.in);


    System.out.print("Please enter your string: ");
    String str = input.nextLine();
    System.out.print("Please enter your word you would like to find: ");
    String word = input.nextLine(); // takes input from user


      int o = str.indexOf(word); // indexOf can use words

    if (o >= 0)
    {
      System.out.println("FOUND");
    }
    else{
      System.out.println("NOT FOUND"); // condtion which is printed
    }


      
