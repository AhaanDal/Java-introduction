public class Main 
{
 public static void main( String[] args )
  {
    int age = 20;

    if (age > 18){
      System.out.print("You are allowed to drive");
    }
    else if (age == 18){
     System.out.print("You are allowed to drive ");
   } 
    else{
      System.out.print("You are not allowed to drive");
    }
    

    
    }
}
